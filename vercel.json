{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "outputDirectory": "public",
    "functions": {
      "api/lambda.php": {
        "runtime": "vercel-php@0.7.4"
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/lambda.php"
      }
    ]
  }